#include<iostream>
#include <stdio.h>
using namespace std;;
void swap(int,int);
int main()
{
int num1, num2 ;
cout<<"Enter Value Of A :: ";
cin>>num1;
cout<<" \nEnter Value of B :: ";
cin>>num2;
cout<<"Before Swapping, Value of ::NUm1 = "<<num1<<" NUM2 ="<<num2<<endl;
swap (num1, num2) ;


}

void swap(int num1, int num2)
{
	int temp;
	temp=num1;
	num1=num2;
	num2=temp;
	cout<<" After Swapping, Value of ::NUM1 = "<<num1<<" NUM2 = "<<num2<<endl;
}
