#include<iostream>
#include<stdio.h>
using namespace std;;
int swap (int &num1, int &num2)
{
int temp;
temp=num1;
num1=num2;
num2=temp;

return 0;
}

int main()
{
int a=5, b=10;
	cout<<"\nBefore swapping"<<"A ="<<a<<endl<<" B = "<<b<<endl;
swap(a, b);
	cout<<"\n After swapping"<<" A ="<<a<<endl<<"B = "<<b<<endl;
return 0;
}
