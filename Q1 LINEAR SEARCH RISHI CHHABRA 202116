//LINEAR SEARCH RISHI CHHABRA 202116

#include<iostream>
using namespace std;   
int main()
{
    int a[10], i, num,m;
    cout<<"Enter 5 Numbers: ";
    for(i=0; i<5; i++)
        cin>>a[i];
    cout<<"Enter a Number to Search: ";
    cin>>num;
    for(i=0; i<5; i++)
    {
        if(a[i]==num)
        
            {
                m = i;
                break;
            }
    }
    cout<<"Found at POSITION No."<<m+1;
    cout<<endl;
    return 0;
}
